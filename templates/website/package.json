{
  "name": "@payloadcms/template-website",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dev:prod": "rm -rf .next && pnpm build && pnpm serve",
    "generate:types": "payload generate:types",
    "ii": "pnpm --ignore-workspace install",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "payload": "payload",
    "reinstall": "rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "seed": "rm -rf media && cross-env PAYLOAD_SEED=true PAYLOAD_DROP_DATABASE=true next dev",
    "serve": "next start"
  },
  "dependencies": {
    "@lexical/list": "^0.14.5",
    "@lexical/react": "0.14.5",
    "@lexical/rich-text": "^0.14.5",
    "@lexical/utils": "0.14.5",
    "@payloadcms/db-mongodb": "3.0.0-beta.30",
    "@payloadcms/db-postgres": "3.0.0-beta.30",
    "@payloadcms/live-preview-react": "3.0.0-beta.30",
    "@payloadcms/next": "3.0.0-beta.30",
    "@payloadcms/plugin-cloud": "3.0.0-beta.30",
    "@payloadcms/plugin-nested-docs": "3.0.0-beta.30",
    "@payloadcms/plugin-redirects": "3.0.0-beta.30",
    "@payloadcms/plugin-seo": "3.0.0-beta.30",
    "@payloadcms/richtext-lexical": "3.0.0-beta.30",
    "@payloadcms/richtext-slate": "3.0.0-beta.30",
    "@payloadcms/ui": "3.0.0-beta.30",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "graphql": "^16.8.1",
    "jsonwebtoken": "9.0.1",
    "next": "14.3.0-canary.28",
    "payload": "3.0.0-beta.30",
    "payload-admin-bar": "^1.0.6",
    "qs": "6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.45.4",
    "react-router-dom": "5.3.4",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.1.6",
    "@payloadcms/eslint-config": "^1.1.1",
    "@swc/core": "1.3.76",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.9",
    "@types/node": "18.11.3",
    "@types/qs": "^6.9.8",
    "@types/react": "18.2.74",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.6",
    "prettier": "^3.0.3",
    "slate": "0.91.4",
    "ts-node": "10.9.1",
    "typescript": "^5.4.2"
  },
  "packageManager": "yarn@1.22.21",
  "overrides": {
    "@types/react": "18.2.74"
  }
}
