{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./dist/index.d.ts",
  "compiler": {
    "tsconfigFilePath": "./tsconfig.json",
    "overrideTsconfig": {
      "compilerOptions": {
        "baseUrl": ".",
        "paths": {
          "@payloadcms/translations": ["../translations/dist/exports/index.d.ts"],
          "@payloadcms/translations/utilities": ["../translations/dist/exports/utilities.d.ts"]
        }
      }
    }
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "./dist/index.bundled.d.ts"
  },
  "apiReport": { "enabled": false },
  "docModel": { "enabled": false },
  "tsdocMetadata": { "enabled": false },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": { "logLevel": "warning" }
    }
  }
}
