{
  "compilerOptions": {
    "noEmit": true,
    "rootDir": ".",
    "declaration": true,
    "declarationMap": true,
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "types": [
      "jest",
      "node",
      "@types/jest"
    ],
    "incremental": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@payload-config": [
        "./_community/config.ts"
      ]
    }
  },
  "exclude": [
    "dist",
    "build",
    "node_modules",
    "eslint.config.js",
    "dist/**/*.js",
    "**/dist/**/*.js"
  ],
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "setup.js"
  ],
  "references": []
}
