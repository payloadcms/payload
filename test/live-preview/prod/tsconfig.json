      "@payloadcms/ui/hooks/*": ["../../../packages/ui/src/hooks/*.ts"],
      "@payloadcms/ui/icons/*": ["../../../packages/ui/src/icons/*/index.tsx"],
      "@payloadcms/ui/providers/*": ["../../../packages/ui/src/providers/*/index.tsx"],
