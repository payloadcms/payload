{
  "compilerOptions": {
    "target": "esnext",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": false, // Undo this
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
  },
  "exclude": [
    "src/**/*.test.ts"
  ]
}
