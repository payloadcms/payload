# Google Cloud Platform Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCS_BUCKET_NAME=your-bucket-name
GCP_KEY_FILE=./service-account-key.json

# Payload CMS Configuration
PAYLOAD_SECRET=your-secret-key-here
PAYLOAD_PUBLIC_SERVER_URL=http://localhost:3000

# Database Configuration
# PostgreSQL (recommended)
DATABASE_URI=postgresql://user:password@localhost:5432/payload

# MongoDB (alternative)
# DATABASE_URI=mongodb://localhost:27017/payload

# MCP Server Configuration
PAYLOAD_API_URL=http://localhost:3000
PAYLOAD_API_KEY=your-api-key

# Optional: Cloud SQL
CLOUD_SQL_CONNECTION_NAME=project:region:instance

# Optional: Environment
NODE_ENV=development
PORT=3000
